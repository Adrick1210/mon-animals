<!DOCTYPE html>
<html lang="en">
<%- include("../partials/head.ejs") %>
<body>
    <%- include("../partials/header.ejs") %>
    <main>
        <div>
          <% for (let animal of animals) { %>
          <article>
            <a href="/animals/<%= animal._id %>">
              <h2>
                <%= animal.species %> - <%= animal.extinct ? "Not Around" : "Still Around" %>
              </h2>
            </a>
          </article>
          <% } %>
        </div>
      </main> 
</body>
</html>