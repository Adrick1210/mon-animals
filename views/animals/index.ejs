<!DOCTYPE html>
<html lang="en">
  <%- include("../partials/head.ejs") %>
  <body>
    <%- include("../partials/header.ejs") %>
    <a href="/animals/new" class="btn btn-primary" role="button">Add Animal</a>
    <main>
      <div>
        <% for (let animal of animals) { %>
        <div class="container">
          <a href="/animals/<%= animal._id %>">
            <h2>
              <%= animal.species %> - <%= animal.extinct ? "Not Around" : "Still Around" %>
            </h2>
          </a>
        </div>
        <% } %>
      </div>
    </main>
  </body>
</html>
